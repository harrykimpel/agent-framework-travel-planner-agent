# OpenAI API Configuration
#OPENAI_API_KEY=your_openai_api_key_here

# Azure OpenAI Configuration
#AZURE_OPENAI_API_ENDPOINT=https://your-resource-name.openai.azure.com/
#AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
#AZURE_OPENAI_API_VERSION=2024-02-15-preview
#AZURE_OPENAI_MODEL_NAME=gpt-4o
#AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name_here

# Google Gemini Configuration
#GEMINI_API_KEY=your_gemini_api_key_here

# GitHub Models Configuration
#GITHUB_TOKEN=your_github_token_here

# AWS Bedrock Configuration
#AWS_REGION=us-east-1
# Note: AWS credentials should be configured via AWS CLI or environment variables
# export AWS_ACCESS_KEY_ID=your_key_id
# export AWS_SECRET_ACCESS_KEY=your_secret_key

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=False
FLASK_APP=app.py

# Weather API
#OPENWEATHER_API_KEY=your_openweather_api_key_here

# New Relic (optional)
OTEL_SERVICE_NAME=travel-planner-web
#NEW_RELIC_ENTITY_GUID=your_entity_guid
#NEW_RELIC_ACCOUNT=your_account_name
#NEW_RELIC_ACCOUNT_ID=your_account_id
#NEW_RELIC_TRUSTED_ACCOUNT_ID=your_trusted_account_id

# OpenTelemetry OTLP Exporter Configuration
# Agent Framework uses OTLP_ENDPOINT (different from standard OTEL_ variables)
OTLP_ENDPOINT=https://otlp.nr-data.net
#OTLP_ENDPOINT=https://otlp.eu01.nr-data.net
OTEL_EXPORTER_OTLP_ENDPOINT=https://otlp.nr-data.net
#OTEL_EXPORTER_OTLP_ENDPOINT=https://otlp.eu01.nr-data.net
OTEL_EXPORTER_OTLP_HEADERS=api-key=YOUR_NEW_RELIC_LICENSE_KEY
OTEL_TRACES_EXPORTER=otlp
OTEL_METRICS_EXPORTER=otlp
OTEL_LOGS_EXPORTER=otlp
OTEL_INSTRUMENTATION_GENAI_CAPTURE_MESSAGE_CONTENT=true
OTEL_INSTRUMENTATION_GENAI_CAPTURE_MESSAGE_METADATA=true
OTEL_INSTRUMENTATION_GENAI_CAPTURE_TOOL_OUTPUT=true
OTEL_INSTRUMENTATION_GENAI_CAPTURE_TOOL_INPUT=true
OTEL_PYTHON_LOGGING_AUTO_INSTRUMENTATION_ENABLED=true